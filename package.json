{
  "name": "melann-lending",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:performance": "jest tests/performance --testTimeout=60000",
    "test:all": "npm run test && npm run test:performance",
    "analyze": "ANALYZE=true next build",
    "deploy": "npm run build && git add . && git commit -m \"Auto-deploy: $(date)\" && git push && netlify deploy --prod --dir=.next",
    "deploy:vercel": "npm run build && git add . && git commit -m \"Auto-deploy: $(date)\" && git push && vercel --prod",
    "deploy:netlify": "npm run build && git add . && git commit -m \"Auto-deploy: $(date)\" && git push && netlify deploy --prod --dir=.next"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.1",
    "lucide-react": "^0.525.0",
    "next": "^16.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lighthouse": "^12.7.1",
    "netlify-cli": "^23.15.1",
    "puppeteer": "^24.11.2",
    "tailwindcss": "^4",
    "typescript": "^5.9.3",
    "web-vitals": "^5.1.0"
  }
}
